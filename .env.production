# Server Configuration
PORT=3001
NODE_ENV=production

# Security
API_KEY=your_secure_api_key_here
CORS_ORIGIN=chrome-extension://your_extension_id_here

# Media Configuration
MEDIA_PATH=/media
JELLYFIN_MOVIES_PATH=/media/movies
JELLYFIN_SHOWS_PATH=/media/shows

# Download Settings
DOWNLOAD_DIR=/downloads
COMPLETED_DIR=/completed
MAX_CONCURRENT_DOWNLOADS=2

# Video Quality Settings
DEFAULT_VIDEO_QUALITY=best
DEFAULT_VIDEO_TYPE=movie

# Logging
LOG_LEVEL=info

# API Rate Limiting
API_RATE_LIMIT=100
API_RATE_WINDOW_MS=900000  # 15 minutes 